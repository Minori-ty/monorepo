    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.1",
    "typescript": "^4.9.3",
